{
  "name": "SFSFExtension",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@graphql-tools/schema": "^10.0.23",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "^3",
    "@sap-cloud-sdk/resilience": "^3",
    "@sap/cds": "^8",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "cds-swagger-ui-express": "^0.10.0",
    "cors": "^2.8.5",
    "express": "^4",
    "express-graphql": "^0.12.0",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "ECPersonalInformation": {
        "kind": "odata-v2",
        "model": "srv/external/ECPersonalInformation",
        "[development]": {
          "credentials": {
            "requestTimeout": 30000,
            "url": "https://sandbox.api.sap.com/successfactors/odata/v2",
            "authentication": "NoAuthentication"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "sfsfDest"
          }
        }
      },
      "ECTimeOff": {
        "kind": "odata-v2",
        "model": "srv/external/ECTimeOff",
        "[development]": {
          "credentials": {
            "requestTimeout": 30000,
            "url": "https://sandbox.api.sap.com/successfactors/odata/v2",
            "authentication": "NoAuthentication"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "sfsfDest"
          }
        }
      }
    },
    "preview": {
      "ui5": {
        "version": "1.97.0",
        "theme": "sap_horizon"
      }
    },
    "features": {
      "preserve_computed": true,
      "kibana_formatter": true,
      "audit_personal_data": true,
      "optimized_search": true,
      "rest_new_adapter": true,
      "graphql": true
    }
  }
}